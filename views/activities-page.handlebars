<head>
    <link rel="stylesheet" href="../public/styles/activities-page.css" />
</head>
<div class="my-activities">
    <section class="image-header">
        <header class="">
            <img src="../public/images/iron-stride-header.png" alt="iron-stride-header">
        </header>
    </section>
        {{!-- Insert the user's workouts below --}}
        {{#each workoutData as |workout|}}
        <div class="workouts">
            <h1>{{workout.name}}</h1>
            <p>Date: {{workout.date}}</p>
            <p>Elapsed Time: {{workout.time}}</p>
            <p>Workout Distance: {{workout.distance}}{{workout.unit}}</p>
            <p>Calories burned: {{workout.calories}}</p>
            <p></p>
        </div>
        {{/each}}
    </div>
</div>